@model IEnumerable<ApplicationWeb.Models.categorie>
@{
    ViewBag.Title = "create";
    Layout = "~/Views/Layouts/_Default.cshtml";
}
<div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">
		<div class="row">
			<ol class="breadcrumb">
				<li><a href="#">
					<em class="fa fa-home"></em>
				</a></li>
				<li class="active">Categorie</li>
			</ol>
		</div><!--/.row-->
		<div class="row">
			<div class="col-lg-12">
				<h1 class="page-header">Nouvelle Categorie</h1>
			</div>
		</div><!--/.row-->
    <div class="row">
        <div class="col-md-12>
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()
                <div class="form-group">
                    <label for="NomCategorie">Nom de la categorie</label>
                    @Html.EditorFor(model=>model.NomCategorie,new {HtmlAttributes= new {@class="form-control form-control-sm"} })
                </div>
                <button type="submit" class=" btn btn-sm btn-primaty">Ajouter</button>
            }
         </div>
    </div>

